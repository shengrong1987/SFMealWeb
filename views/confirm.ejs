  <div id="meal-confirm-container" class="padding-clear" data-center="<%= meal.delivery_center%>" data-range="<%= meal.delivery_range %>" data-user="<%= user.id || null%>">

    <div class="row">
      <div class="col-md-8">
        <div class="box white">
          <div class="row">
            <div class="col-xs-12">
              <div class="box-compact black">
                <span class="text-left text-white">1.<%= __('pickup-method') %></span>
              </div>
            </div>
          </div>

          <h3></h3>

          <div class="row">
            <div class="col-xs-12">
              <h4 class="text-left"><u><%= __('choose-method') %></u>
                <% if(meal.isPartyMode){%>
                <img class="pop-icon" style="top: -10px;right:-10px;" src="/images/group-icon-<%= locale%>.png">
                <%}%>
              </h4>
            </div>
          </div>

          <h4></h4>

          <div class="row">
            <div class="col-xs-12">
              <div class="btn-set text-left" id="method" data-toggle="btn-set">
                <% if(meal.isPartyMode){%>
                <button data-toggle="tab" href="#deliveryTab" class="btn btn-disable btn-fat btn-lg active disabled" style="margin-right: 10px;" value="delivery"><strong><%= __('delivery') %></strong></button>
                <%}else{%>
                <%if(meal.isDelivery){%>
                <button data-toggle="tab" href="#pickupTab" class="btn btn-disable btn-fat btn-lg active disabled" style="margin-right: 10px;" value="pickup"><strong><%= __('self-pickup') %></strong></button>
                <button data-toggle="tab" href="#deliveryTab" class="btn btn-disable btn-fat btn-lg disabled" style="margin-right: 10px;" value="delivery"><strong><%= __('delivery') %></strong></button>
                <%}else if(meal.isShipping){%>
                <button data-toggle="tab" href="#pickupTab" class="btn btn-disable btn-fat btn-lg disabled" style="margin-right: 10px;" value="pickup"><strong><%= __('self-pickup') %></strong></button>
                <button data-toggle="tab" href="#shippingTab" class="btn btn-disable btn-fat btn-lg active disabled" value="shipping"><strong><%= __('shipping') %></strong></button>
                <%}else{%>
                <button data-toggle="tab" href="#pickupTab" class="btn btn-disable btn-fat btn-lg active disabled" style="margin-right: 10px;" value="pickup"><strong><%= __('self-pickup') %></strong></button>
                <%}}%>
              </div>
            </div>
          </div>

          <h3></h3>
          <div class="row">
            <div class="col-sm-6">
              <div class="tab-content" id="googleMapAlignView">
                <div id="pickupTab" class="tab-pane <%= meal.isPartyMode === true ? '' : 'active'%>">
                  <div class="row text-left">
                    <div class="col-xs-12">
                      <h4 class="text-left"><u><%= __('choose-pickup-location') %></u></h4>
                    </div>
                    <div class="col-xs-12">
                      <div class="radio-box" id="pickupMethod" data-user-lat="<%= user.lat%>" data-user-long="<%= user.long%>">
                        <%
                        var i = 0;
                        var j = i+1;
                        if(meal.pickups){
                          colors = ['red', 'blue', 'green','pink','yellow','orange','dark-blue'];
                          meal.pickups.forEach(function(pickup){ if(pickup.method == 'pickup' && !meal.isPartyMode){ %>
                          <label class="btn btn-outline-light vertical-inline option pickupOption" data-time="<%= meal.dispatchingHour(pickup.pickupFromTime, pickup.pickupTillTime)%>" data-color="<%= colors[i]%>" data-location="<%= pickup.publicLocation || pickup.location%>">
                            <input type="radio" id="pickup-1-<%= i%>" data-index="<%= pickup.index%>" name="pickup-location-set" class="regular-radio" <%= i==0 ? "checked=''" : ''%>><label for="pickup-1-<%= i%>"></label>
                            <span class="content">
                              <i class="fa fa-hand-paper-o cursor-pointer text-<%= colors[i]%>" data-toggle="tooltip" title="<%= __('pickup')%>"></i>
                              <% if( (user.orders && user.orders.some(function(order){ return order.meal == meal.id})) || (user && user.host == meal.chef.id)){%>
                              <span class="location" data-value="<%= pickup.location%>"> <%= pickup.location%></span>
                              <%}else{%>
                              <span class="location" data-value="<%= pickup.publicLocation || pickup.location%>"> <%= pickup.publicLocation || pickup.location%></span>
                              <%}%>
                              <br/>
                              <span class="pickupInstruction" data-value="<%= pickup.comment%>"><strong><%= __('pickup-instruction') %></strong>:&nbsp; <%= pickup.comment || ''%></span>
                              <div class="vertical-align" style="margin-top:15px;">
                                <div class="pickup-date" style="width: 70px;">
                                  <span class="col-xs-12"><%= meal.getMonthName(pickup.pickupFromTime)%> <%= meal.getDateFromDate(pickup.pickupFromTime)%></span>
                                  <span class="col-xs-12"><%= __(meal.getWeekFromDate(pickup.pickupFromTime))%></span>
                                </div>
                                <%= meal.dispatchingHour(pickup.pickupFromTime, pickup.pickupTillTime)%>
                              </div>
                            </span>
                          </label>
                        <%}i++;j++;})}; %>
                      </div>
                    </div>

                    <% if(user.id){%>

                    <div class="col-xs-12" style="margin-top: 10px;">
                      <h4 class="text-left"><u><%= __('choose-contact-info') %></u></h4>
                    </div>

                    <%}%>

                    <div class="col-xs-12">
                      <div class="radio-box">
                        <%
                        var firstname = user.firstname;
                        var addresses = user.address;
                        if(addresses && addresses.length != 0){
                          keys = Object.keys(addresses);
                          for(var i=0; i < keys.length; i++){
                            var key = keys[i];
                            var addressObj = addresses[key];
                            var phone = addressObj.phone;
                        %>
                        <label class="btn btn-default fixed-label vertical-align option contactOption">
                          <input type="radio" id="pickup-2-<%= i%>" name="pickup-contact-set" data-color="red" data-index="<%= i%>" class="regular-radio" <%= phone == user.phone ? "checked=''" : ''%>><label for="pickup-2-<%= i%>"></label>
                          <span style="margin-left: 10px;"><%= firstname + "+" + phone %></span>
                        </label>
                        <%}}else{phone = user.phone;if(phone){ %>
                        <label class="btn btn-default fixed-label vertical-align option contactOption">
                          <input type="radio" id="pickup-2-<%= i%>" name="pickup-contact-set" data-index="<%= i%>" class="regular-radio" checked=''><label for="pickup-2-<%= i%>"></label>
                          <span style="margin-left: 10px;"><%= firstname + "+" + phone %></span>
                        </label>
                        <%}} %>
                        </div>
                    </div>
                  </div>
                </div>

                <div id="deliveryTab" class="tab-pane <%= meal.isPartyMode === true ? 'active' : ''%>">

                  <div class="row text-left">

                    <div class="col-xs-12">
                      <h4 class="text-left"><strong><%= __('choose-delivery-option') %></strong></h4>
                    </div>

                    <div class="col-xs-12">
                      <div class="radio-box" id="deliveryMethod">
                        <%
                        colors = ['red', 'blue', 'green','pink','yellow','orange','dark-blue'];
                        i= 0;j = 0;
                        if(meal.pickups){ meal.pickups.forEach(function(pickup){ if(pickup.method == 'delivery'){ if((pickup.isDateCustomized && !meal.isPartyMode) || (!pickup.isDateCustomized && meal.isPartyMode) ){return} %>
                        <label class="btn btn-outline-light option deliveryOption" data-time="<%= meal.dispatchingHour(pickup.pickupFromTime, pickup.pickupTillTime)%>" data-area="<%= pickup.area || '' %>" data-color="<%= colors[i]%>" data-center="<%= pickup.deliveryCenter%>">
                          <input type="radio" id="delivery-2-<%= i%>" data-index="<%= pickup.index%>" name="delivery-contact-set" class="regular-radio" <%= j==0 ? "checked=''" : ''%>><label for="delivery-2-<%= i%>"></label>
                          <i class="fa fa-truck cursor-pointer text-<%= colors[i]%>" data-toggle="tooltip" title="<%= __('delivery')%>"></i>
                          <span> <%= pickup.area || pickup.deliveryCenter%> <%= __('nearby')%></span>
                          <% if(!meal.isPartyMode){%>
                          <div class="vertical-align" style="margin-top:15px;">
                            <div class="pickup-date" style="width: 70px;">
                              <span class="col-xs-12"><%= meal.getMonthName(meal.pickups[0].pickupFromTime)%> <%= meal.getDateFromDate(meal.pickups[0].pickupFromTime)%></span>
                              <span class="col-xs-12"><%= __(meal.getWeekFromDate(meal.pickups[0].pickupFromTime))%></span>
                            </div>
                            <%= meal.dispatchingHour(pickup.pickupFromTime, pickup.pickupTillTime)%>
                          </div>
                          <%}%>
                        </label>
                        <%j++;}i++;})}; %>
