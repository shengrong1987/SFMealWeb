<div class="container default-container text-center relative" style="margin-top:-20px;">

  <div class="row">
    <div class="banner" style="width: 100%;background-color: #1c1c1c;">
      <div class="img-wrapper">
        <img src="/images/homecover-1.jpg" class="relative middle" style="width: 1024px;">
      </div>

      <div class="absolute logo-container">
        <div class="relative text-center" style="top: 80px;">
          <a href="/" class="logo-middle"></a>
        </div>

        <div class="relative text-center search-container" style="top: 90px;">
          <h1 class="text-white"><strong><%= __('welcome')%></strong></h1>
        </div>
      </div>

      <div class="text-align relative" style="bottom: 100px;">
        <button class="btn btn-red btn-lg red middle" style="width: 200px;font-size: large;" onclick="location.href='/meal'"><%= __('see-menu')%></button>
      </div>
    </div>
  </div>

  <div class="row">
    <div class="white middle" style="max-width: 1280px;margin-top:-60px;">

      <div class="middle bar">

        <h1 style="padding: 30px;"></h1>

        <div class="vertical-align">
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-8 col-sm-4 col-md-2">
            <label style="font-size: x-large;"><%= __('landing-title-1-1')%><br/><%= __('landing-title-1-2')%></label>
          </div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
        </div>

        <h1 style="padding: 20px;"></h1>

        <div class="cell with-text-left" style="margin: 20px 0 20px 0;">
          <div class="box-cell middle">
            <img src="/images/landing_1.jpg" style="width: 100%;">
            <div style="margin: 26px 0 13px 0;">
              <p class="text-black"><b><%= __('landing-user-1-1')%></b></p>
              <p class="text-grey"><%= __('landing-user-1-2')%><br/><%= __('landing-user-1-3')%></p>
              <div class="divider none-margin black" style="width: 50px;height: 5px;"></div>
            </div>
          </div>
        </div>

        <div class="cell with-text-left" style="margin: 20px 0 20px 0;">
          <div class="box-cell middle">
            <img src="/images/landing_2.jpg" style="width: 100%;">
            <div style="margin: 26px 0 13px 0;">
              <p class="text-black"><b><%= __('landing-user-2-1')%></b></p>
              <p class="text-grey"><%= __('landing-user-2-2')%><br/><%= __('landing-user-2-3')%></p>
              <div class="divider none-margin black" style="width: 50px;height: 5px;"></div>
            </div>
          </div>
        </div>

        <div class="cell with-text-left" style="margin: 20px 0 20px 0;">
          <div class="box-cell box-cell-left">
            <img src="/images/landing_3.jpg" style="width: 100%;">
            <div style="margin: 26px 0 13px 0;">
              <p class="text-black"><b><%= __('landing-user-3-1')%></b></p>
              <p class="text-grey"><%= __('landing-user-3-2')%><br/><br/><%= __('landing-user-3-3')%></p>
              <div class="divider none-margin black" style="width: 50px;height: 5px;"></div>
            </div>
          </div>
        </div>

        <div class="cell with-text-left" style="margin: 20px 0 20px 0;">
          <div class="box-cell box-cell-left">
            <img src="/images/landing_4.jpg" style="width: 100%;">
            <div style="margin: 26px 0 13px 0;">
              <p class="text-black"><b><%= __('landing-user-4-1')%></b></p>
              <p class="text-grey"><%= __('landing-user-4-2')%><br/><br/><%= __('landing-user-4-3')%></p>
              <div class="divider none-margin black" style="width: 50px;height: 5px;"></div>
            </div>
          </div>
        </div>

      </div>

      <div style="overflow: hidden;">

        <h1 style="padding: 20px;"></h1>

        <div class="row bar">
          <div class="col-sm-4 text-right">
            <div class="row">
              <div class="hovereffect-2 left pull-right">
                <img class="img-responsive cell" src="/images/landing_5.jpg" style="width:100%;">
                <div class="overlay">
                  <h2></h2>
                  <p>
                    <a href="/meal?keyword=entree"><%= __('see-more')%></a>
                  </p>
                </div>
              </div>
            </div>
          </div>
          <div class="col-sm-4">
            <div class="row black cell">
              <div class="box-primary text-white">
                <h1><%= __('landing-preorder-2-1')%></h1>
                <p><%= __('landing-preorder-2-2')%></p>
                <div>
                  <button class="btn btn-outline btn-lg border-white-strong" onclick="javascript:location.href='/meal/search?type=preorder'" style="margin-top: 50px;"><b><%= __('view-preorder')%></b></button>
                </div>
              </div>
            </div>
          </div>
          <div class="col-sm-4 text-left">
            <div class="row">
              <div class="hovereffect-2 pull-left">
                <img class="img-responsive cell" src="/images/landing_6.jpg" style="width:100%;">
                <div class="overlay">
                  <h2></h2>
                  <p>
                    <a href="/meal?keyword=dessert"><%= __('see-more')%></a>
                  </p>
                </div>
              </div>
            </div>
          </div>
        </div>

        <h1 style="padding: 20px;"></h1>

        <div class="vertical-align">
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-6 col-sm-4 col-md-2">
            <label style="font-size: x-large;"><%= __('landing-title-3')%></label>
          </div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
        </div>

        <h1 style="padding: 30px;"></h1>

        <div class="row bar">
          <% if(hosts){
            var index = 0;
            hosts.forEach(function(host){ index++; if(index <= 3){%>
          <div class="col-sm-4">
            <div class="box">
              <img class="round_thumb" style="width: 100px;" src="<%= host.picture%>" onclick="javascript:location.href='/host/<%= host.id%>'">
              <p style="margin-top: 15px;"><%= host.intro%></p>
              <div data-toggle="tooltip" data-placement="bottom" title="<%= host.score.toFixed(2)%>">
                <div class="rating" data-toggle="star-set" data-rate="<%= host.score.toFixed(2)%>">
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                  <i class="fa fa-star"></i>
                </div>
              </div>
              <a href="/host/<%= host.id%>"><%= __('view-dish')%></a>
            </div>
          </div>
          <%}})}%>
        </div>

        <h1 style="padding: 30px;"></h1>

        <div class="vertical-align">
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-6 col-sm-4 col-md-2">
            <label style="font-size: x-large;"><%= __('landing-title-4')%></label>
          </div>
          <div class="col-xs-2">
            <div class="divider grey"></div>
          </div>
          <div class="col-xs-offset-1 col-sm-offset-2 col-md-offset-3"></div>
        </div>

        <div class="row bar">
          <div class="col-xs-12">
            <div class="box-primary">
              <p><%= __('landing-sfmeal')%></p>
              <button class="btn btn-outline border-black-strong" style="margin-top: 20px;"><b><%= __('i-cook')%></b></button>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>

</div>

<script>
  setupLanguage();
</script>
