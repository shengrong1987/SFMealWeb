            var mealObj = meals.meals[dateKey];
            var actualMeals = mealObj.meals;
            actualMeals.forEach(function(meal) {meal.dishes.forEach(function(dish) {if(!dishes.includes(dish.id)){dishes.push(dish.id);%>
              <tr class="success dish item px-2" data-id=<%= dish.id %> data-left-amount=<%= meal.leftQty[dish.id]%> style="display: none;">
                <td><%= dish.titleI18n(locale) %><br/><span class="preference"></span></td>
                <td class="price" value=<%= dish.price %> data-extra=0>$<%= dish.price %></td>

                <td class="input-group input-group-sm amount-input round ml-auto pl-0" style="max-width:200px;min-width:30px;" data-toggle="amount-input" data-customClickHandle="true" data-max="<%= meal.leftQty[dish.id]%>" data-dish="<%= dish.id%>">
                  <div class="input-group-prepend minus" onclick="javascript:orderFood('<%= dish.id %>',-1,true)">
                    <span class="input-group-text">-</span>
                  </div>
                  <input class="form-control text-center amount" type="number" pattern="[0-9]*" inputmode="numeric" value="0" readonly>
                  <div class="input-group-append add" onclick="javascript:orderFood('<%= dish.id %>',1,true)">
                    <span class="input-group-text">+</span>
                  </div>
                </td>
              </tr>
          <% }});})});%>

          </tbody>
